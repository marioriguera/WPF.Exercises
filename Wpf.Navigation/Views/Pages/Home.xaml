<UserControl x:Class="Wpf.Navigation.Views.Pages.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wpf.Navigation.Views.Pages" xmlns:viewsmodels="clr-namespace:Wpf.Navigation.ViewsModels" d:DataContext="{d:DesignInstance Type=viewsmodels:HomeViewModel}"
             mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Chocolate">
        <StackPanel VerticalAlignment="Center"
                    HorizontalAlignment="Center">
            <TextBlock Text="Home"
                       Foreground="Aqua"
                       FontSize="50"/>
            <Button Content="Settings"
                    Height="20"
                    Command="{Binding SettingsNavigateCommand}"/>
            <Button Content="Users"
                    Height="20"
                    Command="{Binding UsersNavigateCommand}"/>
        </StackPanel>
    </Grid>
</UserControl>
